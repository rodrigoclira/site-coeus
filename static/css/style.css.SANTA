*{
	margin:0;
	padding:0;
	border:none;
}

body{
	margin:0;

	background: rgb(28,151,178); /* Old browsers */
	background: -moz-linear-gradient(-45deg,  rgba(28,151,178,1) 0%, rgba(39,115,132,1) 17%, rgba(14,67,85,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right bottom, color-stop(0%,rgba(28,151,178,1)), color-stop(17%,rgba(39,115,132,1)), 	color-stop(100%,rgba(14,67,85,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(-45deg,  rgba(28,151,178,1) 0%,rgba(39,115,132,1) 17%,rgba(14,67,85,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(-45deg,  rgba(28,151,178,1) 0%,rgba(39,115,132,1) 17%,rgba(14,67,85,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(-45deg,  rgba(28,151,178,1) 0%,rgba(39,115,132,1) 17%,rgba(14,67,85,1) 100%); /* IE10+ */
	background: linear-gradient(135deg,  rgba(28,151,178,1) 0%,rgba(39,115,132,1) 17%,rgba(14,67,85,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1c97b2', endColorstr='#0e4355',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */



}
.page{
	min-height:628px;
}

/*------------------------- Font ------------------------*/ 
@font-face {
    font-family: 'RokkittRegular';
    src: url('rokkitt-regular-webfont.eot');
    src: url('rokkitt-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('rokkitt-regular-webfont.woff') format('woff'),
         url('rokkitt-regular-webfont.ttf') format('truetype'),
         url('rokkitt-regular-webfont.svg#RokkittRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

/*------------------------- Header ------------------------*/ 
.header{
	width:900px;
	margin:0 auto;
	height:90px;
	padding:10px 30px 10px 30px;
}
.header a#logo{
	display:block;
	float:left;
	outline:none;
}
.header a#logo img{
	border:0;
	
}
.header ul{
	margin:0;
	list-style:none;
	float:left;
	position: relative;
	top: 30px;
	left: 45px;
}
.header ul li{
	float:left;
	margin:0 0 0 50px;
}
.header ul li.selected a,.header ul li a:hover{
	text-shadow:0 0 8px #FFFFFF;
}
.header ul li a{
	font-size:11pt;
	text-decoration:none;
	font-weight:400;
	color:#FFF;
	font-family: Helvitica, Arial, Verdana, sans-serif;
	outline:none;
	text-shadow: 0 2px #000000;
}

/*------------------------- Body ------------------------*/ 
.body{
	width:900px;
	margin:0 auto;
	padding:30px;
	overflow:hidden;
	background-color:#FFF;
	border: 1px solid #000;
	
	-webkit-border-radius: 5px;
    border-radius: 5px;
	
	 -webkit-box-shadow:  0px 0px 5px 2px rgba(0, 0, 0, 0.5);
     box-shadow:  0px 0px 5px 2px rgba(0, 0, 0, 0.5);
		
}

.body ul.blog{
	list-style:none;		
	overflow:hidden;
}
.body ul.blog li:first-child{
	background-color:#FFF;
	padding:0;
	margin-left:0;

}
.body ul.blog li{
	float:left;
	background-color:#FFF;
	margin:20px 0 0 28px;
	padding:0;

}
.body ul.blog li div{
	width:281px;
	text-align:center;
	padding: 8px 0 20px 0;
	float:none;
	margin:0;
	background:none;
}
.body ul.blog li div a img{	
	margin:0 0 0 0;
	height: 168px;
	width: 255px;
	overflow:hidden;
	
}
.body ul.blog li div a {
	cursor:default;
}
.body ul.blog li div p{
	width:258px;
	margin:17px 0 0 10px;
	padding:0;
}
.body ul.blog li div a:first-child{
	margin:0;
	padding:0;
	background:none;
	width:auto;
}
.body ul.blog li div a:hover{
