.body .smallRigthColumn ul#iconList{
	text-align:left;
	display: inline-block;
	position:relative;
	top: -38px;
	left:3px;
	padding:2px;
	background-color:#F4F5F5;
	border-radius: 5px;
	height: 25px;
	opacity:0.8;
}

.body .smallRigthColumn ul#iconList li{
	float:left;
	margin-left:5px;
}
.body .smallRigthColumn ul#iconList li:first-child{
	margin-left:0;
}

.body .smallRigthColumn ul#iconList li img{
	margin:0;
}

.body .smallRigthColumn a#quemSomosLink{
	margin: 20px 0 0 0;
	display:inline-block;	
	text-decoration:none;
	padding:5px;
	color:#198AAD;
	font-size:8pt;
}
.body .smallRigthColumn a#quemSomosLink:hover{
	text-decoration:underline;
}

/*------------------------- Footer ------------------------*/ 
.footer{
	width:940px;
	padding:20px 10px 20px;
	margin:0 auto;
	overflow:hidden;
	text-align:center;
}
.footer ul{
	list-style:none;
	margin:0 0 0 60px;
	padding:0;
	overflow:hidden;
	float:left;
}
.footer ul li:first-child a{
	border:none;
	padding:0 6px 0 0;
}
.footer ul li{
}

.footer ul li a{
	border-width: 1px;
	border-color:#5b5b5b;
	border-style: none none none solid;
	color:#5b5b5b;
	font-size:9pt;
	text-decoration:none;
	padding:0 6px;
	outline:none;
	text-align:center;
}
.footer p{
	color:#FFF;
	margin: 5px 0 2px 0;
	font-size:9pt;
	font-family: Helvitica, Verdana, Arial, sans-serif;
}
p#icoLinks{
	height:30px;
	width:auto;
}

p#icoLinks a{
	margin-right: 10px;

.footer div#Contato{
	display:inline-block;
	width: 200px;	
}
